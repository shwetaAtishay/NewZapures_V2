<style>

   .noc-trackings .order-tracking {
        width: 100%;
        position: relative;
        display: flex;
        align-items: center;
        gap: 20px;
        padding: 15px;
    }

   .noc-trackings .order-tracking .is-complete {
            display: block;
            position: relative;
            border-radius: 50%;
            height: 30px;
            width: 30px;
            border: 0px solid #AFAFAF;
            background-color: #f7be16;
            margin-right: 20px;
            transition: background 0.25s linear;
            -webkit-transition: background 0.25s linear;
            z-index: 2;
        }

   .noc-trackings .order-tracking .is-complete:after {
                display: block;
                position: absolute;
                content: '';
                height: 14px;
                width: 7px;
                top: -2px;
                bottom: 0;
                left: 5px;
                margin: auto 0;
                border: 0px solid #AFAFAF;
                border-width: 0px 2px 2px 0;
                transform: rotate(45deg);
                opacity: 0;
            }

   .noc-trackings .order-tracking.completed .is-complete {
            border-color: #27aa80;
            border-width: 0px;
            background-color: #27aa80;
        }

   .noc-trackings .order-tracking.completed .is-complete:after {
                border-color: #fff;
                border-width: 0px 3px 3px 0;
                width: 7px;
                left: 11px;
                opacity: 1;
            }

   .noc-trackings .order-tracking p {
            color: #A4A4A4;
            font-size: 16px;
            margin-top: 8px;
            margin-bottom: 0;
            line-height: 20px;
            width: 100%;
        }

   .noc-trackings .order-tracking p span {
                font-size: 14px;
            }

   .noc-trackings .order-tracking.completed p {
            color: #000;
        }

   .noc-trackings .order-tracking::before {
            content: '';
            display: block;
            height: calc(100% - 0px);
            width: 3px;
            background-color: #f7be16;
            top: -25px;
            position: absolute;
            left: calc(10px + 20px);
            z-index: 0;
        }

   .noc-trackings .order-tracking:first-child:before {
            display: none;
        }

   .noc-trackings .order-tracking.completed:before {
            background-color: #27aa80;
        }
</style>


<div class="card p-0 m-0 border-0 noc-layouts">
    <div class="card-header title-heading p-0" style="display: flex !important; align-items: baseline !important; place-content: space-between !important;">
        <h4 class="mt-0 title-heading float-left">@ViewBag.Title</h4>
        @*<a href="@Url.Action("EditApplication", "Trustee", new {applGUID=@draftAppl.ApplGuid})" class="btn btn-sm btn-gradient-dark"><i class="fa fa-arrow-left"></i> Back To Edit Application</a>*@
    </div>
    <div class="card-body p-0">
        <div class="noc-layouts-tbl">
            <table class="table table-bordered table-striped">
                <tr>
                    <th style="color:black!important;font-weight:500">Trust Name :</th>
                    <td colspan="3">g.r.trust</td>

                    @*<td hidden>@draftAppl.TrustName</td>*@

                </tr>
                <tr>
                    <th style="color:black!important;font-weight:500">College Name :</th>
                    <td colspan="3">@draftAppl.CollegeName</td>

                </tr>
                <tr>
                    <th style="color:black!important;font-weight:500">Course</th>
                    <td>@draftAppl.CourseName</td>

                    <th style="color:black!important;font-weight:500">Department Name</th>
                    <td>Collage Education</td>
                    @*<td>@draftAppl.DepartmentName</td>*@
                </tr>
            </table>

        </div>
        <div class="title-heading rounded-0 my-2 w-100 float-left">Applied Subject Details</div>
        <div id="DivSubjectInfo" class="w-100 float-left" style="padding: 20px 0;">
            <div class="noc-layouts-tbl">
                <table class="table table-borderless table-responsive-md" id="tblSubjectInfo">
                    <thead class="sticky">
                        <tr>
                            <th scope="col">Sr No</th>
                            <th scope="col" hidden>Subjid</th>
                            <th scope="col">Subject Name</th>
                            <th scope="col">Department Name</th>
                            <th scope="col">Course</th>
                            @*<th scope="col">CollageName</th>*@
                        </tr>
                    </thead>
                    <tbody>
                        @{

                            if (subjectList != null)
                            {

                                var i = 1;
                                foreach (var item in subjectList)
                                {
                                    <tr>
                                        <td>@i</td>
                                        <td scope="col" hidden>@item.iPK_SubId</td>
                                        <td scope="col">@item.SubjectName</td>
                                        <td scope="col">@item.TagDegrees</td>
                                        <td scope="col">@item.TagCourse</td>
                                        @*<td scope="col">@item.CollegeName</td>*@
                                    </tr>
                                    i++;
                                }


                            }
                        }
                    </tbody>
                </table>
            </div>

        </div>
        <div class="title-heading rounded-0 my-2 w-100 float-left">Staff Details</div>
        <div id="DivStaffInfo" class="w-100 float-left" style="padding: 20px 0;">
            <div class="noc-layouts-tbl">
                <table class="table table-borderless table-responsive-lg" id="tblStaffInfo">
                    <thead class="sticky">
                        <tr>
                            <th>SNo</th>
                            <th>Name</th>
                            <th>Role</th>
                            <th>Mobile</th>
                            <th>Email</th>
                            <th>Qualification</th>
                            <th>Type</th>
                            <th style="text-align:center">Aadhaar</th>
                            <th style="text-align:center">Pan</th>
                            <th style="text-align:center">Profile</th>
                            <th style="text-align:center">Experience</th>
                        </tr>
                    </thead>
                    <tbody>
                        @if (stafflist != null)
                        {
                            int i = 1;
                            foreach (var item in stafflist)
                            {
                                <tr>
                                    <td>@i</td>
                                    <td>@item.Name</td>
                                    <td>@item.Role</td>
                                    <td>@item.Mobile</td>
                                    <td>@item.Email</td>
                                    <td>@item.Qualification</td>
                                    <td>@item.Type</td>
                                    <td style="text-align:center">
                                        @if (item.Aadhaar != "")
                                        {<a href="@Url.Action("DownloadDocuments", "Trustee", new { id = item.Aadhaar })" target="_blank" alt="Red dot"><i class="fa fa-download"></i></a>}
                                </td>
                                <td style="text-align:center">
                                    @if (item.Aadhaar != "")
                                    {<a href="@Url.Action("DownloadDocuments", "Trustee", new { id = item.Pan })" target="_blank" alt="Red dot"><i class="fa fa-download"></i></a>}
                                </td>
                                <td style="text-align:center">
                                    @if (item.Aadhaar != "")
                                    {<a href="@Url.Action("DownloadDocuments", "Trustee", new { id = item.Profile })" target="_blank" alt="Red dot"><i class="fa fa-download"></i></a>}
                                </td>
                                <td style="text-align:center">
                                    @if (item.Aadhaar != "")
                                    {<a href="@Url.Action("DownloadDocuments", "Trustee", new { id = item.Experience })" target="_blank" alt="Red dot"><i class="fa fa-download"></i></a>}
                                </td>

                            </tr>
                            i++;
                        }
                    }
                    else
                    {
                        <tr>
                            <td colspan="8" style="text-align: center;">No Data Available</td>
                        </tr>
                    }
                    </tbody>
                </table>
            </div>
        </div>
        <div class="title-heading rounded-0 my-2 w-100 float-left">Land and Building Information</div>
        <div id="DivLandInfo" class="w-100 float-left" style="padding:20px 0;">
            <div class="noc-layouts-tbl">
                <table class="table table-borderless" id="tblLandInfo">
                    <thead>
                        <tr>
                            <th>#</th>
                            <th>Land Situated</th>
                            <th>Land Doc Type</th>

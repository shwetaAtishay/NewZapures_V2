                            <th>Land Area</th>
                            <th>Total Area</th>
                            <th>Converted Doc</th>
                            <th>Land Title Doc</th>
                            <th>Land Doc</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <td colspan="13" class="text-center">No Data Available</td>
                        </tr>
                    </tbody>
                </table>
            </div>

        </div>



        <div class="title-heading rounded-0 my-2 w-100 float-left">College Fee Structure</div><br />
        <div id="DivCollageFeeList" class="w-100 float-left" style="padding: 20px 0;">
            <div class="noc-layouts-tbl">
                <table class="table table-borderless table-responsive-sm" id="tblCollageFeeInfo">
                    <thead class="sticky">
                        <tr>
                            <th>S.No.</th>
                            <th>Fees</th>
                            <th>Rate</th>
                        </tr>
                    </thead>
                    <tbody>
                        @if (FeeList.rateLists != null)
                        {
                            int i = 1;
                            foreach (var item in FeeList.rateLists)
                            {
                                if (item.IsSelect == true)
                                {
                                    <tr>
                                        <td>@i</td>
                                        <td>
                                            @item.RateName
                                        </td>
                                        <td>
                                            @item.Rate
                                        </td>
                                    </tr>
                                }
                                i++;
                            }
                        }
                        else
                        {
                            <tr>
                                <td colspan="8" style="text-align: center;"> No Data Available</td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
        <div class="title-heading rounded-0 my-2 w-100 float-left">Architectrual Details</div>
        <div class="hidden mt-3 w-100 float-left" id="ArchitectureId">        </div><br />
        <div class="title-heading rounded-0 my-2 w-100 float-left">Other Facilites</div>
        <div id="DivCollageFacility" class="w-100 float-left" style="padding: 20px 0;">
            <div class="noc-layouts-tbl">
                <table class="table table-borderless table-responsive-sm" id="tblCollageFacility">
                    <thead class="sticky">
                        <tr>
                            <th>SNo</th>
                            <th>Facility Name</th>
                            <th>Image</th>
                        </tr>
                    </thead>
                    <tbody>
                        @if (CollageFacilityMster != null)
                        {
                            int i = 1;
                            foreach (var item in CollageFacilityMster.list)
                            {
                                if (item.IsSelect == true)
                                {
                                    <tr>
                                        <td>@i</td>
                                        <td>

                                            @item.FacilityName

                                        </td>
                                        <td>
                                            @if (item.uploadFile != "")
                                            {
                                                <img src="data:@item.ContentType;base64,@item.uploadFile" alt="profile-user" class="rounded-circle" width="50" height="50" />
                                            }
                                        </td>
                                    </tr>
                                }
                                i++;
                            }
                        }
                        else
                        {
                            < tr >
                            < td colspan = "8" style = "text-align: center;" > No Data Available</ td >

                            </ tr >
                        }
                    </tbody>
                </table>
            </div>
        </div>
        <div class="title-heading rounded-0 my-2 w-100 float-left">Acadmy Information</div>
        <div id="DivAcadmicInfo" class="w-100 float-left" style="padding: 20px 0;">
            <div class="noc-layouts-tbl">
                <table class="table table-borderless table-responsive-lg" id="tblAcadmicInfo">
                    <thead class="sticky">
                        <tr>
                            <th scope="col">Sr No</th>
                            <th scope="col">From Year</th>
                            <th scope="col">To Year</th>
                            <th scope="col">No Of Student Appear</th>
                            <th scope="col">Result</th>
                            <th scope="col">No Of Student Passed</th>
                            <th scope="col">Percentage</th>
                        </tr>
                    </thead>
                    <tbody>
                        @{

                            if (acdmcs != null)
                            {
                                if (acdmcs.Count <= 0)
                                {
                                    <tr>
                                        <td colspan="6" rowspan="1" style="text-align:center">No Data Available</td>
                                    </tr>
                                }
                                else
                                {
                                    var i = 1;
                                    foreach (var item in acdmcs)
                                    {
                                        <tr>
                                            <td>@i</td>
                                            <td scope="col">@item.FromYear</td>
                                            <td scope="col">@item.ToYear</td>
                                            <td scope="col">@item.NoOfStudent</td>
                                            <td scope="col">@item.Result</td>
                                            <td scope="col">@item.NoOfStudentPass</td>
                                            <td scope="col">@item.Percentage</td>
                                        </tr>
                                        i++;
                                    }

                                }
                            }
                        }
                    </tbody>
                </table>
            </div>

        </div>
    </div>
</div>



        function BindUniversities(DeptID) {
            var applicationModal = {
                "deptID": parseInt(DeptID)
            }

            $.ajax({
                headers: {
                    'Accept': 'application/json',
                    'Content-Type': 'application/json'
                },
                url: GetglobalDomain() + "/BasicDetails/GetUniversityForDept",
                dataType: 'json',
                type: 'Post',
                cache: false,
                data: JSON.stringify(applicationModal),
                success: function (data) {
                    //console.log(data);
                    var htmlString = `<option value="">--University --</option>`;
                    var mainData = data.Data;
                    console.log(mainData);

                    if (mainData.length > 0) {
                        mainData.forEach(function (item) {
                            htmlString += `<option value=${item.Id}>${item.Text}</option>`;
                        })
                    }

                    $('#txtUniverSity').html('');
                    $('#txtUniverSity').html(htmlString);

                },
                error: function (d) {
                    console.log(d);
                }
            });

        }


